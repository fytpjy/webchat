page {
  background-color: #f5f5f5;
  height: 100%;
  font-family: 'microsoft yahei',Verdana,Arial,Helvetica,sans-serif;
}
.cl{
  zoom: 1;
}
.cl:after{
  content: " ";
  height: 0;
  clear: both;
  visibility: hidden;
  display: block;
}
.lf{float: left}
.rt{float: right}
.btnBox {
  width: 100%;
  padding: 40rpx 30rpx;
  box-sizing: border-box;
}
.wxBtn {
  width: 100%;
  background-color: #44cc84;
  font-size: 34rpx;
  text-align: center;
  color: #fff;
}
/*.wxBtn:active {*/
  /*background-color: #2fae6b;*/
  /*box-shadow: inset 0 0 20rpx rgba(0, 0, 0, 0.3);*/
/*}*/
.belowBtn{
  width: 100%;
  background-color: #44cc84;
  font-size: 34rpx;
  text-align: center;
  color: #fff;
  position: fixed;
  bottom: 0;
  left: 0;
  border-radius: 0;
  z-index: 10;
}
.belowBtn:after{
  border: none;
}
.fullBtn{
    background: #ccc;
    width: 100%;
    font-size: 34rpx;
    text-align: center;
    color: #fff;
}
.whBtn {
  width: 100%;
  background-color: #fff;
  font-size: 34rpx;
  text-align: center;
  color: #333;
    border: 1rpx solid #d5d5d5;
}
/*.whBtn:active {*/
  /*box-shadow: inset 0 0 20rpx rgba(0, 0, 0, 0.3);*/
/*}*/
.joinactive{
  padding: 0 .8rem;
  background: #FFF;
}
.joinactive .li{
  border-bottom: 1rpx solid #d9d9d9;
}
.joinactive:last-child .li{
    border: none;
}
.joinactive .a_info{
    padding: 30rpx 0;
    box-sizing: border-box;
}
.joinactive .a_info .cont{
    font-size: 32rpx;
    line-height: 40rpx;
    margin-bottom: 10rpx;
    color: #333;
    width: 100%;
    box-sizing: border-box;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    word-wrap: normal;
    display: block;
}
.joinactive .a_info .more .dt{
  line-height: 40rpx;
}
.joinactive .a_info .more .dt:last-child{
    padding-top: 10rpx;
    position: relative;
    margin-top: 10rpx;
}
.joinactive .a_info .more .dt .l{
  float: left;
}
.joinactive .a_info .more .dt .l .icon_03{
    width: 32rpx;
    height: 33rpx;
    vertical-align: middle;
}
.joinactive .a_info .more .dt .l .icon_02{
  width: 31rpx;
  height: 31rpx;
  vertical-align: middle;
}
.joinactive .a_info .more .dt .r{
  float: right;
}
.joinactive .a_info .more .dt .r .icon_01{
  width: 25rpx;
  height: 31rpx;
  vertical-align: middle;
}
.joinactive .a_info .more .dt .r .btn{
  background: #ccc;
  color: #FFF;
  font-size: 26rpx;
  width: 150rpx;
  border-radius: 5rpx;
  padding: 0;
  line-height: 54rpx;
  text-align: center;
    position: absolute;
    right: 0;
    bottom: 0;
}
.joinactive .a_info .more .dt text{
  font-size: 26rpx;
  color: #999;
  margin-left: 10rpx;
}
.joinactive .book{
    width: 100%;
  border-top: 1rpx solid #d9d9d9;
  line-height: 90rpx;
}
.joinactive .book .icon_11{
  width: 32rpx;
  height: 34rpx;
  vertical-align: middle;
}
.joinactive .book text{
  font-size: 32rpx;
  color: #333;
  margin-left: .3rem;
}
.joinactive .member{
  overflow: hidden;
  border-top: 1rpx solid #d9d9d9;
  padding: 30rpx 0;
}
.joinactive .member .l{
  float: left;
}
.joinactive .member .t{
  line-height: 40rpx;
}
.joinactive .member .t .icon_05{
  width:36rpx;
  height: 35rpx;
  vertical-align: middle;
}
.joinactive .member .t text{
  font-size: 32rpx;
  color: #333;
  margin-left: .3rem;
}
.joinactive .member .b{
  float: left;
}
.joinactive .member .b text{
  color: #929292;
  font-size: 28rpx;
}
.joinactive .member .r{
  float: right;
}
.joinactive .member .r button{
  background: #FFF;
  border: 1rpx solid #44cc84;
  color: #44cc84;
  font-size: 26rpx;
  width: 150rpx;
  height: 54rpx;
  padding: 0;
  line-height: 54rpx;
  margin-top:24rpx;
  border-radius: 5rpx;
  outline: none;
}
.nodata{
    text-align: center;
}
.nodata .icon_07{
    width: 349rpx;
    height: 153rpx;
    margin-top: 106rpx;
}
.nodata .t{
    color: #b2b2b2;
    font-size:28rpx;
    margin-bottom: 100rpx;
}
button{
  line-height: 80rpx;
}
button:after{
  border: none;
}
.share {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.6);
  text-align: right;
  z-index: 100;
}
.share image {
  margin-right: 30rpx;
  margin-top: 10rpx;
  width: 448rpx;
  height: 195rpx;
}
.navigator-hover{
    background: transparent;
    opacity: 1;
}