.cityselect .h2{
    background: #ebebeb;
    line-height: 2rem;
    font-size: .9rem;
    color: #333;
    padding-left: .6rem;
}
.cityselect .list{
    background: #f5f5f5;
    overflow: hidden;
    display: flex;
    flex-flow: row wrap;
    text-align: center;
    padding: 0 1rem .9rem;
}
.cityselect .list .li{
    float: left;
    width: 25%;
    margin-top: .9rem;
}
.cityselect .list .li view{
    background: #FFF;
    color: #333;
    font-size: .9rem;
    padding: .3rem 0;
    width: 84%;
    margin: auto;
}
.cityselect .list .li view:active{
    background: #eee;
}